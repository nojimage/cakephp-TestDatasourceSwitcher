{
    "name": "nojimage/cakephp-test-datasource-switcher",
    "description": "TestDatasourceSwitcher plugin for CakePHP 3",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "TestDatasourceSwitcher\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TestDatasourceSwitcher\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
